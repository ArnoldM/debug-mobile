{
  "name": "debug-mobile",
  "version": "0.0.1",
  "description": "Mobile Application",
  "engines": {
    "node": ">=20.12.2",
    "npm": ">=10.5.2"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve",
    "start:prod": "ng serve --configuration production",
    "build:dev": "ng build --configuration development",
    "build:prod": "ng build --configuration production && npm run transloco:optimize",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e:dev": "playwright test --ui",
    "lint": "ng lint",
    "format": "npx prettier 'src/**/*.{ts,html,scss}' --write",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find",
    "transloco:optimize": "transloco-optimize www/assets/i18n",
    "storybook": "ng run app:storybook",
    "build-storybook": "ng run app:build-storybook",
    "test-storybook": "test-storybook --url http://localhost:6006",
    "test-storybook:watch": "test-storybook --url http://localhost:6006 --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.3",
    "@angular/common": "^18.0.3",
    "@angular/compiler": "^18.0.3",
    "@angular/core": "^18.0.3",
    "@angular/forms": "^18.0.3",
    "@angular/platform-browser": "^18.0.3",
    "@angular/platform-browser-dynamic": "^18.0.3",
    "@angular/router": "^18.0.3",
    "@angular/service-worker": "^18.0.3",
    "@capacitor/app": "6.0.0",
    "@capacitor/core": "6.0.0",
    "@capacitor/haptics": "6.0.0",
    "@capacitor/keyboard": "6.0.0",
    "@capacitor/status-bar": "6.0.0",
    "@ionic/angular": "^8.0.0",
    "@jsverse/transloco": "7.1.0",
    "@jsverse/transloco-locale": "^7.0.0",
    "@ngrx/effects": "^18.0.0",
    "@ngrx/entity": "^18.0.0",
    "@ngrx/operators": "^18.0.0",
    "@ngrx/router-store": "^18.0.0",
    "@ngrx/signals": "^18.0.0-rc.1",
    "@ngrx/store": "^18.0.0",
    "@ngrx/store-devtools": "^18.0.0",
    "ionicons": "^7.4.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.5.0",
    "@analogjs/vite-plugin-angular": "^1.3.0",
    "@analogjs/vitest-angular": "^1.5.0",
    "@angular-devkit/build-angular": "^18.0.4",
    "@angular-eslint/builder": "^18.0.1",
    "@angular-eslint/eslint-plugin": "^18.0.1",
    "@angular-eslint/eslint-plugin-template": "^18.0.1",
    "@angular-eslint/schematics": "^18.0.1",
    "@angular-eslint/template-parser": "^18.0.1",
    "@angular/cli": "^18.0.4",
    "@angular/compiler-cli": "^18.0.3",
    "@angular/language-service": "^18.0.3",
    "@capacitor/cli": "6.0.0",
    "@chromatic-com/storybook": "^1.4.0",
    "@compodoc/compodoc": "^1.1.24",
    "@faker-js/faker": "^8.4.1",
    "@ionic/angular-toolkit": "^11.0.1",
    "@jsverse/transloco-keys-manager": "^4.1.0",
    "@jsverse/transloco-optimize": "^7.0.0",
